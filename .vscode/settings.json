{
    "dotnet.defaultSolution": "src/csharp/LeetCode.sln",
    "cSpell.words": [
        "devcontainers",
        "nums",
        "Tribonacci"
    ],
    "python.analysis.extraPaths": [
        "./src",
        "./src/python/problems",
        "./src/python/problems/pandas"
    ],
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./src/python/problems",
        "-p",
        "*test.py"
    ],
    "python.envFile": "${workspaceFolder}/src/python/.env",
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true
}